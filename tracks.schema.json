{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "Playlist": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "album": {
            "type": "string"
          },
          "artist": {
            "type": "string"
          },
          "list": {
            "type": "string"
          },
          "title": {
            "type": "string"
          }
        },
        "required": ["album", "artist", "list", "title"]
      }
    }
  },
  "required": ["Playlist"]
}
